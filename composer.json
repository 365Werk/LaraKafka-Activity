{
    "name": "werk365/larakafka-activity",
    "description": "Spatie Activitylogger extention for Lara Kafka",
    "license": "MIT",
    "version": "1.0.2",
    "authors": [
        {
            "name": "Hergen Dillema",
            "email": "h.dillema@365werk.nl",
            "homepage": "https://365werk.nl"
        }
    ],
    "homepage": "https://github.com/Werk365/larakafka",
    "keywords": ["Laravel", "LaraKafka"],
    "require": {
        "werk365/larakafka":  "^1.0",
        "spatie/laravel-activitylog": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Werk365\\LaraKafkaActivity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Werk365\\LaraKafkaActivity\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Werk365\\LaraKafkaActivity\\LaraKafkaActivityServiceProvider"
            ]
        }
    }
}
